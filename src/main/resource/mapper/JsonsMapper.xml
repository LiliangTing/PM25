<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liliangliang.core.dao.JsonsDAO">
	<sql id="insertSql">
		jsontext,type
	</sql>
	<insert id="insert">
		insert into jsons(
		<include refid="insertSql" />
		)values
		( #{jsontext},#{type}
		)


	</insert>

</mapper>